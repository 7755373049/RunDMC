<Project xmlns       ="http://developer.marklogic.com/site/internal"
         xmlns:form  ="http://developer.marklogic.com/site/internal/form"
         xmlns:label ="http://developer.marklogic.com/site/internal/form/attribute-labels"
         xmlns:values="http://developer.marklogic.com/site/internal/form/values">
  <form:fieldset legend="Edit">
    <name        form:label="Project Name"
                 form:wide="yes"/>
    <contributors><!-- href="">-->
      <contributor form:repeating="yes"
                   form:heading="DOWNLOAD LINK(S)"
                   form:label="Author"/>
    </contributors>
    <description form:label="Description"
                 form:type="textarea"/>
  </form:fieldset>
  <form:fieldset legend="Download Link(s)">
    <versions><!-- get-involved-href="">-->
      <version href=""           label:href          ="Link"
               server-version="" label:server-version="Version needed" values:server-version="4.0-1 4.1 4.2"
               date=""           label:date          ="Date"           form:repeating="yes"/>
    </versions>
  </form:fieldset>
  <top-threads form:hide="yes"/>
</Project>
